<h3>Welcome to Stack Overflow</h3>

<% if current_user %>
  <h5><%= link_to 'Ask a New Question', new_question_path %></h5>
<% end %>

<ul>
  <%= @questions.each do |question| %>
    <li><%= question.content %>, Asked by: <%= question.user.name %></li>
  <% end %>
</ul>



<% if current_user %>
  <%= link_to "Log Out", log_out_path %>
<% else %>
  <%= link_to "Create an Account", new_user_path %>
  <%= link_to "Log In", log_in_path %>
<% end %>
